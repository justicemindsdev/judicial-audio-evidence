{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "audio_evidence_exact_timestamps/*.mp3",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/professional",
      "dest": "/professional_legal_dashboard.html"
    },
    {
      "src": "/complete",
      "dest": "/complete_22_segments_dashboard.html"
    },
    {
      "src": "/final-exact",
      "dest": "/final_exact_timed_dashboard.html"
    },
    {
      "src": "/final-working",
      "dest": "/final_working_audio_dashboard.html"
    },
    {
      "src": "/supreme-court",
      "dest": "/supreme_court_opening.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}