{
  "version": 2,
  "name": "judicial-audio-evidence",
  "builds": [
    {
      "src": "final_working_audio_dashboard.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/final_working_audio_dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/final_working_audio_dashboard.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
EOF < /dev/null